{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "window": true,
    "document": true,
    "jQuery": true
  },
  "rules": {
    "semi": "warn",
    "no-console": "warn",
    "no-undef": "warn",
    "no-unused-vars": "warn"
  }
}
